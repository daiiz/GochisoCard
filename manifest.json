{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "images/128x128.png",
         "38": "images/128x128.png"
      },
      "default_popup": "popup.html",
      "default_title": "ごちそうカード に登録"
   },
   "options_page": "options.html",
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/; object-src 'self'",
   "description": "ごちそう写真のコレクション",
   "icons": {
      "128": "images/128x128.png",
      "48": "images/48x48.png",
      "16": "images/128x128.png"
   },
   "manifest_version": 2,
   "name": "ごちそうカード",
   "content_scripts": [
    {
      "matches": ["http://miil.me/g/*", "http://www.gochisophoto.com/*/*"],
      "js": ["ctx/content_script_v2.js"],
      "run_at": "document_end"
    }
   ],
   "permissions": ["storage", "http://*/*", "https://*/*", "tabs"],
   "version": "0.3.22"
}